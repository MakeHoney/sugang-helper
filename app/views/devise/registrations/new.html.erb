 <body class = "test3" id = "test4"> 
 <div class="limiter">
  <div class="container-login100">
    <div class="wrap-login100 p-t-30 p-b-50">
    <span class="login100-form-title p-b-41">
					회원가입
				</span>
    
    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= f.error_notification %>
     <div class="login100-form validate-form p-b-33 p-t-5">
        
      <div class="wrap-input100 validate-input " data-validate = "Enter username">
        <%= f.input :email, label: "이메일", required: true, autofocus: true, input_html: {class:"input100 ",name:"text",type:"email",placeholder:"Email" } %>
        <span class="focus-input100" data-placeholder="&#xe82a;"></span>
      </div>
      
      <div class="wrap-input100 validate-input " data-validate = "Enter password">
        <%= f.input :password, label: "비밀번호", required: true,  input_html: {class:"input100",name:"pass",type:"password",placeholder:"Password"} %>
        <span class="focus-input100" data-placeholder="&#xe80f;"></span>
      </div>
      
       <div class="wrap-input100 validate-input " data-validate = "Enter password">
        <%= f.input :password_confirmation, label: "비밀번호 확인", input_html: {class:"input100",name:"pass",type:"password",placeholder:"Password"}, rrequired: true %>
        <span class="focus-input100" data-placeholder="&#xe80f;"></span>
      </div>
    
      <div class="form-actions container-login100-form-btn m-t-32">
        <%= f.button :submit, "회원가입" %>
      </div>
    
    </div>
   </div>
  </div>
</div>
<% end %>
  <div id="dropDownSelect1"></div>
	
